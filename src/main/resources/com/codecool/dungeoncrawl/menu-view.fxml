<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="279.0" prefWidth="272.0" styleClass="anchor-pane" stylesheets="@menu.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.codecool.dungeoncrawl.game.controller.MenuController">
   <children>
      <Label layoutX="31.0" layoutY="46.0" prefHeight="35.0" prefWidth="212.0" styleClass="menu-title-label" stylesheets="@menu.css" text="Dungeon Crawl" textAlignment="JUSTIFY" textFill="WHITE">
         <font>
            <Font name="Consolas Bold" size="29.0" />
         </font>
      </Label>
      <GridPane layoutY="110.0" prefHeight="169.0" prefWidth="272.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="85.0" minWidth="10.0" prefWidth="75.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="125.0" minWidth="10.0" prefWidth="125.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="92.0" minWidth="10.0" prefWidth="75.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
