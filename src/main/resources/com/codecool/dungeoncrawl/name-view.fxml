<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="249.0" prefWidth="266.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.codecool.dungeoncrawl.logic.controller.NameController">
   <children>
      <Label fx:id="titleLabel" layoutX="46.0" layoutY="44.0" text="Enter your name">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="tfName" layoutX="59.0" layoutY="112.0" onKeyPressed="#startGame" />
      <Label fx:id="adviceLabel" layoutX="55.0" layoutY="144.0" text="Press enter to start new game" />
   </children>
</AnchorPane>
